#include<stdio.h>
#include<stdlib.h>

int main(){
	
	int num1,num2;
	int resultado;
	
	
	printf("\n insira um numero:");
	scanf("%d",&num1);
	printf("\n insira outro numero:");
	scanf("%d",&num2);
	 
	resultado = num1 + num2;
	
	printf("\n o resultado e %d + %d = %d",num1,num2,resultado);
	
	return 0;
	
}
